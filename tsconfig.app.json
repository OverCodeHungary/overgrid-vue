{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/components/OverGrid/*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "declaration": true,
    "emitDeclarationOnly": false,
    "declarationDir": "dist",
    "outDir": "dist",
    "baseUrl": ".",
    "composite": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}