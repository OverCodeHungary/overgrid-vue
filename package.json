{
  "name": "overgrid-vue",
  "private": false,
  "version": "1.6.5",
  "type": "module",
  "license": "MIT",
  "main": "./dist/overgrid.umd.js",
  "module": "./dist/overgrid.es.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "vue",
    "grid",
    "table",
    "overgrid",
    "datatable",
    "datagrid",
    "crud"
  ],
  "files": [
    "dist",
    "src/components/OverGrid/themes/default.css",
    "src/components/OverGrid/themes/pink.css",
    "src/index.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/overgrid.es.js",
      "require": "./dist/overgrid.umd.js"
    },
    "./dist/overgrid-vue.css": {
      "default": "./dist/overgrid-vue.css"
    }
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "build-typecheck": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:component": "vitest",
    "test:component:ci": "vitest --run --coverage",
    "test:coverage": "vitest --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "npm-publish": "npm run build && npm publish --access=public",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@coders-tm/vue-number-format": "^3.35.4",
    "axios": "^1.10.0",
    "moment": "^2.30.1",
    "vue": "^3.0.0",
    "vue-json-excel3": "^1.0.30"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.2.6",
    "@semantic-release/npm": "^12.0.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@types/vue": "^1.0.31",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "semantic-release": "^24.2.7",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
